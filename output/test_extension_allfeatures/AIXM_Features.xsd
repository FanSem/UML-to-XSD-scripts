            </element>
            <element minOccurs="0" name="precisionApproachGuidance"
                nillable="true" type="aixm:CodeApproachGuidanceType">
                <annotation>
                    <appinfo>
                        <gml:description>Degree to which navigation aids provide accurate approach guidance.  Precision approaches utilize both lateral (course) and vertical (glideslope) information</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="startingElement"
                nillable="true" type="aixm:RunwayElementPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>The runway element is the start of the runway direction.  Typically used to indicate the location of a displaced area.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="availability" nillable="true" type="aixm:ManoeuvringAreaAvailabilityPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>The operational status of the RunwayDirection.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="usedRunway"
                nillable="true" type="aixm:RunwayPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>(DO272/ED99) Each FinalApproachAndTakeOff feature?s attribute idthr shall provide the idrwy object identifier value of the corresponding HelipadThreshold (Rule 15).</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
