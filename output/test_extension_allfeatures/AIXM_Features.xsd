    <group name="AircraftCharacteristicPropertyGroup">
        <sequence>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeAircraftType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicating the type of an aircraft.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="engine" nillable="true" type="aixm:CodeAircraftEngineType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicating the type of aircraft engine.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="numberEngine" nillable="true" type="aixm:CodeAircraftEngineNumberType">
                <annotation>
                    <appinfo>
                        <gml:description>A coded indication for the number of engines of an aircraft.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="typeAircraftICAO"
                nillable="true" type="aixm:CodeAircraftICAOType">
                <annotation>
                    <appinfo>
                        <gml:description>A coded aircraft type designator assigned by ICAO to all aircraft heavier than micro-/ultra-light.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="aircraftLandingCategory"
                nillable="true" type="aixm:CodeAircraftCategoryType">
                <annotation>
                    <appinfo>
                        <gml:description>Classification of aircraft based on 1.3 times stall speed in landing configuration at maximum certified landing mass.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="wingSpan" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <appinfo>
                        <gml:description>The distance between the wing tips of the aircraft.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="wingSpanInterpretation"
                nillable="true" type="aixm:CodeValueInterpretationType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicates whether the Aircraft Characteristic concerns aircraft with larger or smaller wingspan.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="classWingSpan" nillable="true" type="aixm:CodeAircraftWingspanClassType">
                <annotation>
                    <appinfo>
                        <gml:description>A classification of aircraft by wing span dimensions.  It can be used to indicate the size of aircraft that a taxiway can handle.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="weight" nillable="true" type="aixm:ValWeightType">
                <annotation>
                    <appinfo>
                        <gml:description>The maximum aircraft take-off weight.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="weightInterpretation"
                nillable="true" type="aixm:CodeValueInterpretationType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicates whether the Aircraft Characteristic concerns aircraft with bigger or smaller weight.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="passengers" nillable="true" type="aixm:NoNumberType">
                <annotation>
                    <appinfo>
                        <gml:description>The maximum number of passengers</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="passengersInterpretation"
                nillable="true" type="aixm:CodeValueInterpretationType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicates whether the Aircraft Characteristic concerns aircraft with larger or smaller number of passengers.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="speed" nillable="true" type="aixm:ValSpeedType">
                <annotation>
                    <appinfo>
                        <gml:description>A speed (IAS) that can be maintained by the aircraft.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="speedInterpretation"
                nillable="true" type="aixm:CodeValueInterpretationType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicates whether the speed is a maximum or minimum imposed value.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="wakeTurbulence" nillable="true" type="aixm:CodeWakeTurbulenceType">
                <annotation>
                    <appinfo>
                        <gml:description>The wake turbulence category of the aircraft.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="navigationEquipment"
                nillable="true" type="aixm:CodeNavigationEquipmentType">
                <annotation>
                    <appinfo>
                        <gml:description>An indication of the aircraft capability to use a certain ground based, satellite based or on-board system for aerial navigation.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="navigationSpecification"
                nillable="true" type="aixm:CodeNavigationSpecificationType">
                <annotation>
                    <appinfo>
                        <gml:description>A set of aircraft and flight crew requirements needed to support performance-based navigation operations.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="verticalSeparationCapability"
                nillable="true" type="aixm:CodeRVSMType">
                <annotation>
                    <appinfo>
                        <gml:description>An indication of the aircraft capability to maintain a specified vertical separation.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0"
                name="antiCollisionAndSeparationEquipment"
                nillable="true" type="aixm:CodeEquipmentAntiCollisionType">
                <annotation>
                    <appinfo>
                        <gml:description>An indication of the aircraft equipment with collision avoidance systems.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="communicationEquipment"
                nillable="true" type="aixm:CodeCommunicationModeType">
                <annotation>
                    <appinfo>
                        <gml:description>An indication of the aircraft equipment with voice and data communication devices.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="surveillanceEquipment"
                nillable="true" type="aixm:CodeTransponderType">
                <annotation>
                    <appinfo>
                        <gml:description>An indication of the aircraft capability for operating with a certain surveillance system.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
