    <element abstract="true" name="AbstractAirspaceLayerExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="CircleSector" type="aixm:CircleSectorType">
        <annotation>
            <appinfo>
                <gml:description>A group of properties that help describe a 3D pattern based on start angle, stop angle, inner distance, outer distance and upper/lower altitude.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="CircleSectorType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:CircleSectorPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractCircleSectorExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="CircleSectorPropertyGroup">
        <sequence>
            <element minOccurs="0" name="arcDirection" nillable="true" type="aixm:CodeArcDirectionType">
                <annotation>
                    <appinfo>
                        <gml:description>Direction indicating clock-wise or counter-clockwise</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="fromAngle" nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <appinfo>
                        <gml:description>Beginning of angle</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="toAngle" nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <appinfo>
                        <gml:description>Ending of angle</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="angleType" nillable="true" type="aixm:CodeBearingType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating the type of angle: magnetic, bearing, VOR radial.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="angleDirectionReference"
                nillable="true" type="aixm:CodeDirectionReferenceType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating a direction with regard to a reference point.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="innerDistance" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <appinfo>
                        <gml:description>Angle sector volume inner limit</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="outerDistance" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <appinfo>
                        <gml:description>Angle sector volume outer limit</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="upperLimit" nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <appinfo>
                        <gml:description>The uppermost altitude or level that is included in the sector.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="upperLimitReference"
                nillable="true" type="aixm:CodeVerticalReferenceType">
                <annotation>
                    <appinfo>
                        <gml:description>The reference surface used for the value of the upper limit. For example, Mean Sea Level, Ground, standard pressure,  etc..</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="lowerLimit" nillable="true" type="aixm:ValDistanceVerticalType">
