                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="CirclingAreaPropertyGroup">
        <sequence>
            <element maxOccurs="unbounded" minOccurs="0"
                name="designSurface" nillable="true" type="aixm:ObstacleAssessmentAreaPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>Identifies the Obstacle Assessment Area for which the Circiling Area is evaluated by.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="aircraftCategory"
                nillable="true" type="aixm:AircraftCharacteristicPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>A different circling area is constructed and evaluated by aircraft category</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="condition" nillable="true" type="aixm:ApproachConditionPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>Obstacle assessment of circling areas can define specific conditions causing restrictions or different minimums.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="approach"
                nillable="true" type="aixm:InstrumentApproachProcedurePropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>Circling is used on approach procedure to safely provide visual guidance to other runways.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="extent"
                nillable="true" type="aixm:SurfacePropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>depicts the geometry for the circling area</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <element abstract="true" name="AbstractCirclingAreaExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="CirclingRestriction"
        substitutionGroup="aixm:AbstractPropertiesWithSchedule" type="aixm:CirclingRestrictionType">
