        <sequence>
            <element minOccurs="0" name="category" nillable="true" type="aixm:CodeCheckpointCategoryType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicates the position of the checkpoint; airborne or ground</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="upperLimit" nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicating the upper limit of the block in which the check should be conducted.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="upperLimitReference"
                nillable="true" type="aixm:CodeVerticalReferenceType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating the reference for a vertical distance.&#xd;
Two series of values exist:&#xd;
1) real distance: from GND, from the MSL, from the WGS-84 ellipsoid&#xd;
2) pressure distance: QFE, QNH, STD.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="lowerLimit" nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicating the lower limit of the block in which the check should be conducted.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="lowerLimitReference"
                nillable="true" type="aixm:CodeVerticalReferenceType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating the reference for a vertical distance.&#xd;
Two series of values exist:&#xd;
1) real distance: from GND, from the MSL, from the WGS-84 ellipsoid&#xd;
2) pressure distance: QFE, QNH, STD.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="altitudeInterpretation"
                nillable="true" type="aixm:CodeAltitudeUseType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicates how the upper and/or lower altitude values should be interpreted.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="distance" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <appinfo>
                        <gml:description>The value of the distance from the checkpoint to the navaid.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="angle" nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <appinfo>
                        <gml:description>The indication of a bearing (at a given point) by the measurement of the angle between the checkpoint and the navaid equipment (VOR)</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="position"
                nillable="true" type="aixm:ElevatedPointPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>Identifies the point of he checkpoint.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="airportHeliport"
                nillable="true" type="aixm:AirportHeliportPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>Identifies the airport for which the checkpoint is established.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
