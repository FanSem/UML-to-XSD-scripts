                        <gml:description>The length of the runway element.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="width" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <appinfo>
                        <gml:description>The width of the runway element.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="gradeSeparation"
                nillable="true" type="aixm:CodeGradeSeparationType">
                <annotation>
                    <appinfo>
                        <gml:description>A code that indicates the placement of the element at a different height from another element.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="extent"
