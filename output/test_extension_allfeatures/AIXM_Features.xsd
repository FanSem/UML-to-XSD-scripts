                <element minOccurs="0"
                    name="finalPathAlignmentPoint_airportReferencePoint"
                    nillable="true" type="aixm:AirportHeliportPropertyType"/>
            </choice>
            <element maxOccurs="1" minOccurs="0"
                name="visualDescentPoint" nillable="true" type="aixm:TerminalSegmentPointPropertyType">
