        </complexContent>
    </complexType>
    <complexType name="SpecialDateTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:SpecialDateTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
