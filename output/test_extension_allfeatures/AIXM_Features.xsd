    <element abstract="true" name="AbstractProcedureTransitionExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="ProcedureTransitionLeg" type="aixm:ProcedureTransitionLegType">
        <annotation>
            <appinfo>
                <gml:description>Characterises a SegmentLeg which is part of a Procedure Transition&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="ProcedureTransitionLegType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
