        substitutionGroup="aixm:AbstractPropertiesWithSchedule" type="aixm:RouteAvailabilityType">
        <annotation>
            <appinfo>
                <gml:description>Information about the operational availability of a route.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="RouteAvailabilityType">
        <complexContent>
            <extension base="aixm:AbstractPropertiesWithScheduleType">
                <sequence>
                    <group ref="aixm:PropertiesWithSchedulePropertyGroup"/>
                    <group ref="aixm:RouteAvailabilityPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
