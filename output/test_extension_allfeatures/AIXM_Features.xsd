                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="visibilityAdjustment"
                nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <appinfo>
                        <gml:description>Adjusted for the corresponding visibility value.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="approachLightingInoperative"
                nillable="true" type="aixm:CodeYesNoType">
