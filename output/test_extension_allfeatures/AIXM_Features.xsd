            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeFlightType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating a specified  type of a flight, such as OAT/GAT.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="rule" nillable="true" type="aixm:CodeFlightRuleType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating a specific flight rule, such as IFR or VFR.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="status" nillable="true" type="aixm:CodeFlightStatusType">
                <annotation>
                    <appinfo>
                        <gml:description>The designation of a special status for a flight (e.g. ""HEAD"" for Head of State, ""STATE"" for State aircraft other than that of the Head of State).</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="military" nillable="true" type="aixm:CodeMilitaryStatusType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating whether it is a military flight.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="origin" nillable="true" type="aixm:CodeFlightOriginType">
