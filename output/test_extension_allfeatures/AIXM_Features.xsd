                <annotation>
                    <appinfo>
                        <gml:description>depicts the attributes that describe the geometry for the restricted area</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="restrictionArea"
                nillable="true" type="aixm:SurfacePropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>depicts the geometry for the restricted area</gml:description>
                    </appinfo>
                </annotation>
            </element>
        </sequence>
    </group>
    <complexType name="CirclingRestrictionPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:CirclingRestriction"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true" name="AbstractCirclingRestrictionExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="EquipmentUnavailableAdjustment" type="aixm:EquipmentUnavailableAdjustmentType">
        <annotation>
            <appinfo>
                <gml:description>Used to describe adjustments to minimums when equipment is not available&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="EquipmentUnavailableAdjustmentType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:EquipmentUnavailableAdjustmentPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractEquipmentUnavailableAdjustmentExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="EquipmentUnavailableAdjustmentPropertyGroup">
        <sequence>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeEquipmentUnavailableType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicates if the specified altitude/height is adjusted for the case when certain equipment is unavailable.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="approachLightingInoperative"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicates if the specified altitude/height is adjusted for the case when the approach lighting is inoperative.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="adjustmentINOPCol" nillable="true" type="aixm:EquipmentUnavailableAdjustmentColumnPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>contains the adjusted values</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="EquipmentUnavailableAdjustmentPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:EquipmentUnavailableAdjustment"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractEquipmentUnavailableAdjustmentExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="EquipmentUnavailableAdjustmentColumn" type="aixm:EquipmentUnavailableAdjustmentColumnType">
        <annotation>
            <appinfo>
                <gml:description>Adjustments that need to be applied in case equipment is inoperative.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="EquipmentUnavailableAdjustmentColumnType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:EquipmentUnavailableAdjustmentColumnPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
