                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="EquipmentUnavailableAdjustmentColumnPropertyGroup">
        <sequence>
            <element minOccurs="0" name="guidanceEquipment"
                nillable="true" type="aixm:CodeApproachType">
