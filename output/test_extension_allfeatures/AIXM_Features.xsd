            <appinfo>
                <gml:description>Describes a (sub)sector used to restrict flight within a circling area.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="CirclingRestrictionType">
        <complexContent>
            <extension base="aixm:AbstractPropertiesWithScheduleType">
                <sequence>
                    <group ref="aixm:PropertiesWithSchedulePropertyGroup"/>
