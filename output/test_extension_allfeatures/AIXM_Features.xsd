            <element maxOccurs="unbounded" minOccurs="0"
                name="runwayDirection" nillable="true" type="aixm:RunwayDirectionPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>Identifies the runway direction on which the arresting gear is used.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <choice>
                <annotation>
                    <appinfo>
                        <gml:description>Extent of the ArrestingGear.</gml:description>
                    </appinfo>
                </annotation>
                <element minOccurs="0" name="extent_surfaceExtent"
                    nillable="true" type="aixm:ElevatedSurfacePropertyType"/>
                <element minOccurs="0" name="extent_curveExtent"
                    nillable="true" type="aixm:ElevatedCurvePropertyType"/>
                <element minOccurs="0" name="extent_pointExtent"
                    nillable="true" type="aixm:ElevatedPointPropertyType"/>
            </choice>
            <element maxOccurs="1" minOccurs="0"
                name="surfaceProperties" nillable="true" type="aixm:SurfaceCharacteristicsPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>Describes the surface characteristics of the arresting gear.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
