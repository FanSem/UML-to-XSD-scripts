                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractPropertiesWithScheduleExtension"/>
                                <element ref="aixm:AbstractProcedureAvailabilityExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="ProcedureAvailabilityPropertyGroup">
        <sequence>
            <element minOccurs="0" name="status" nillable="true" type="aixm:CodeProcedureAvailabilityType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating the status of the procedure for flight planning/operations.</gml:description>
                    </appinfo>
                </annotation>
            </element>
        </sequence>
    </group>
    <complexType name="ProcedureAvailabilityPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:ProcedureAvailability"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractProcedureAvailabilityExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
