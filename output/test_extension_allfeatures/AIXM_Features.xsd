                            <choice>
                                <element ref="aixm:AbstractProcedureTransitionLegExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="ProcedureTransitionLegPropertyGroup">
        <sequence>
            <element minOccurs="0" name="seqNumberARINC" nillable="true" type="aixm:NoSequenceType">
