        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:ChangeOverPointType">
        <annotation>
            <appinfo>
                <gml:description>The distance from the start of the route portion to the position where change over occurs for VOR defined routes.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="ChangeOverPointType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:ChangeOverPointTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="ChangeOverPointTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:ChangeOverPointTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="ChangeOverPointTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:ChangeOverPointTimeSliceType"/>
    <complexType name="ChangeOverPointTimeSliceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:ChangeOverPointPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <sequence>
                                <element ref="aixm:AbstractChangeOverPointExtension"/>
                            </sequence>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="ChangeOverPointPropertyGroup">
        <sequence>
            <element minOccurs="0" name="distance" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <appinfo>
                        <gml:description>The distance from the start of the route portion to position of the change over point.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0"
                name="applicableRoutePortion" nillable="true" type="aixm:RoutePortionPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>The Route Portion that the change over point applies to.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <choice>
                <annotation>
                    <appinfo>
                        <gml:description>Location of the change over point.</gml:description>
                    </appinfo>
                </annotation>
                <element minOccurs="0"
                    name="location_fixDesignatedPoint" nillable="true" type="aixm:DesignatedPointPropertyType"/>
                <element minOccurs="0" name="location_navaidSystem"
                    nillable="true" type="aixm:NavaidPropertyType"/>
                <element minOccurs="0" name="location_position"
                    nillable="true" type="aixm:PointPropertyType"/>
                <element minOccurs="0" name="location_runwayPoint"
                    nillable="true" type="aixm:RunwayCentrelinePointPropertyType"/>
                <element minOccurs="0" name="location_aimingPoint"
                    nillable="true" type="aixm:TouchDownLiftOffPropertyType"/>
                <element minOccurs="0"
                    name="location_airportReferencePoint"
                    nillable="true" type="aixm:AirportHeliportPropertyType"/>
            </choice>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <element abstract="true" name="AbstractChangeOverPointExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="RoutePropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element name="Route" substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:RouteType">
        <annotation>
            <appinfo>
                <gml:description>A specified route designed for channelling the flow of traffic as necessary for the provision of air traffic services, from the end of the take-off and initial climb phase to the commencement of the approach and landing phase.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="RouteType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:RouteTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="RouteTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:RouteTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="RouteTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:RouteTimeSliceType"/>
    <complexType name="RouteTimeSliceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:RoutePropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <sequence>
                                <element ref="aixm:AbstractRouteExtension"/>
                            </sequence>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="RoutePropertyGroup">
        <sequence>
            <element minOccurs="0" name="designatorPrefix"
                nillable="true" type="aixm:CodeRouteDesignatorPrefixType">
                <annotation>
                    <appinfo>
                        <gml:description>A prefix for the route designator, indicating whether the route is upper ATS, reserved for helicopters or reserved for supersonic flights.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="designatorSecondLetter"
                nillable="true" type="aixm:CodeRouteDesignatorLetterType">
                <annotation>
                    <appinfo>
                        <gml:description>The letter of the route designator. Depending on the region, letters may have special significance with regard to the route usage limitations.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="designatorNumber"
                nillable="true" type="aixm:NoNumberType">
                <annotation>
                    <appinfo>
                        <gml:description>The number of the route designator.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="multipleIdentifier"
                nillable="true" type="aixm:CodeUpperAlphaType">
                <annotation>
                    <appinfo>
                        <gml:description>A suffix for the route designator, indicating alternates for homonymous routes (especially for military training routes).</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="locationDesignator"
                nillable="true" type="aixm:TextDesignatorType">
                <annotation>
                    <appinfo>
                        <gml:description>A textual description of the area in which a designated route is situated.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="name" nillable="true" type="aixm:TextNameType">
                <annotation>
                    <appinfo>
                        <gml:description>A free text name by which the route is known.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeRouteType">
                <annotation>
                    <appinfo>
                        <gml:description>Route classification.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="flightRule" nillable="true" type="aixm:CodeFlightRuleType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating the flight rules to be observed by aircraft.&#xd;
For example, VFR and/or IFR.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="internationalUse"
                nillable="true" type="aixm:CodeRouteOriginType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating if the route segment is international or domestic.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="militaryUse" nillable="true" type="aixm:CodeMilitaryStatusType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating whether the route is for exclusive use by military aircraft.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="militaryTrainingType"
                nillable="true" type="aixm:CodeMilitaryTrainingType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating the type of military training activity for which the route has been designed.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="userOrganisation"
                nillable="true" type="aixm:OrganisationAuthorityPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicates the Organisation normally originating activity on a military training route.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <element abstract="true" name="AbstractRouteExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
