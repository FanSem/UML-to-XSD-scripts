                <annotation>
                    <appinfo>
                        <gml:description>A code indicating if it is 'normal' or 'back' azimuth equipment.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="trueBearing" nillable="true" type="aixm:ValBearingType">
                <annotation>
                    <appinfo>
                        <gml:description>The measured angle between the azimuth beam and True North at the azimuth antenna.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="trueBearingAccuracy"
                nillable="true" type="aixm:ValAngleType">
                <annotation>
