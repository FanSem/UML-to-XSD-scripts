    <element abstract="true" name="AbstractDirectFlightExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="DirectFlightClass"
        substitutionGroup="aixm:AbstractDirectFlight" type="aixm:DirectFlightClassType">
        <annotation>
            <appinfo>
                <gml:description>A category of direct flights (DCT), having some particular characteristics, such as a minimum length.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="DirectFlightClassType">
        <complexContent>
            <extension base="aixm:AbstractDirectFlightType">
                <sequence>
                    <group ref="aixm:DirectFlightPropertyGroup"/>
                    <group ref="aixm:DirectFlightClassPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractDirectFlightExtension"/>
                                <element ref="aixm:AbstractDirectFlightClassExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="DirectFlightClassPropertyGroup">
        <sequence>
            <element minOccurs="0" name="exceedLength" nillable="true" type="aixm:ValDistanceType">
                <annotation>
                    <appinfo>
                        <gml:description>A value used to indicate that only Direct Flights (DCT) that exceed this value are concerned.</gml:description>
                    </appinfo>
                </annotation>
            </element>
        </sequence>
    </group>
    <complexType name="DirectFlightClassPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:DirectFlightClass"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true" name="AbstractDirectFlightClassExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="DirectFlightSegment"
        substitutionGroup="aixm:AbstractDirectFlight" type="aixm:DirectFlightSegmentType">
        <annotation>
            <appinfo>
                <gml:description>A direct flight (DCT) between two specified significant points.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="DirectFlightSegmentType">
        <complexContent>
            <extension base="aixm:AbstractDirectFlightType">
                <sequence>
                    <group ref="aixm:DirectFlightPropertyGroup"/>
                    <group ref="aixm:DirectFlightSegmentPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractDirectFlightExtension"/>
                                <element ref="aixm:AbstractDirectFlightSegmentExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="DirectFlightSegmentPropertyGroup">
        <sequence>
            <choice>
                <annotation>
                    <appinfo>
                        <gml:description>Start of a direct flight segment.</gml:description>
                    </appinfo>
                </annotation>
                <element minOccurs="0" name="start_fixDesignatedPoint"
                    nillable="true" type="aixm:DesignatedPointPropertyType"/>
                <element minOccurs="0" name="start_navaidSystem"
                    nillable="true" type="aixm:NavaidPropertyType"/>
                <element minOccurs="0" name="start_position"
                    nillable="true" type="aixm:PointPropertyType"/>
                <element minOccurs="0" name="start_runwayPoint"
                    nillable="true" type="aixm:RunwayCentrelinePointPropertyType"/>
                <element minOccurs="0" name="start_aimingPoint"
                    nillable="true" type="aixm:TouchDownLiftOffPropertyType"/>
                <element minOccurs="0"
                    name="start_airportReferencePoint" nillable="true" type="aixm:AirportHeliportPropertyType"/>
            </choice>
            <choice>
                <annotation>
                    <appinfo>
                        <gml:description>End point of a direct flight segment.</gml:description>
                    </appinfo>
                </annotation>
                <element minOccurs="0" name="end_fixDesignatedPoint"
                    nillable="true" type="aixm:DesignatedPointPropertyType"/>
                <element minOccurs="0" name="end_navaidSystem"
                    nillable="true" type="aixm:NavaidPropertyType"/>
                <element minOccurs="0" name="end_position"
                    nillable="true" type="aixm:PointPropertyType"/>
                <element minOccurs="0" name="end_runwayPoint"
                    nillable="true" type="aixm:RunwayCentrelinePointPropertyType"/>
                <element minOccurs="0" name="end_aimingPoint"
                    nillable="true" type="aixm:TouchDownLiftOffPropertyType"/>
                <element minOccurs="0" name="end_airportReferencePoint"
                    nillable="true" type="aixm:AirportHeliportPropertyType"/>
            </choice>
        </sequence>
    </group>
    <complexType name="DirectFlightSegmentPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:DirectFlightSegment"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true" name="AbstractDirectFlightSegmentExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="FlightConditionCircumstance" type="aixm:FlightConditionCircumstanceType">
        <annotation>
            <appinfo>
                <gml:description>Circumstance related to the elementary condition (such as departing from an aerodrome),  which is used to further refine the  selection of flights being affected by the restriction.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="FlightConditionCircumstanceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:FlightConditionCircumstancePropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractFlightConditionCircumstanceExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="FlightConditionCircumstancePropertyGroup">
        <sequence>
            <element minOccurs="0" name="referenceLocation"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating if the condition element is a reference location for the flight restriction, i.e. if the time schedule associated with the restriction must be considered as applicable for that flight condition element.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="relationWithLocation"
                nillable="true" type="aixm:CodeLocationQualifierType">
                <annotation>
                    <appinfo>
                        <gml:description>A code qualifying a location (such as an airspace, aerodrome, point, etc.) when used as condition element. For example, departing from that location, crossing the location, activation hours of the location are used as condition, etc..</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="FlightConditionCircumstancePropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:FlightConditionCircumstance"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractFlightConditionCircumstanceExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="FlightConditionCombination"
        substitutionGroup="aixm:AbstractPropertiesWithSchedule" type="aixm:FlightConditionCombinationType">
        <annotation>
            <appinfo>
                <gml:description>A combination of conditions involving:&#xd;
1) an operator such as \"AND\", \"OR\", \"AND NOT\", \"SEQUENCE\", \"NONE\" and&#xd;
2) one or more operands, each of these being either a condition element or another combination of conditions.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="FlightConditionCombinationType">
        <complexContent>
            <extension base="aixm:AbstractPropertiesWithScheduleType">
                <sequence>
                    <group ref="aixm:PropertiesWithSchedulePropertyGroup"/>
                    <group ref="aixm:FlightConditionCombinationPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractPropertiesWithScheduleExtension"/>
                                <element ref="aixm:AbstractFlightConditionCombinationExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="FlightConditionCombinationPropertyGroup">
        <sequence>
            <element minOccurs="0" name="logicalOperator"
                nillable="true" type="aixm:CodeFlowConditionOperationType">
                <annotation>
                    <appinfo>
                        <gml:description>The type of operation combining an elementary condition or a previously defined combination with other elementary conditions or previously defined combinations.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="element"
                nillable="true" type="aixm:FlightConditionElementPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>An element of the Flight Condition.</gml:description>
                    </appinfo>
                </annotation>
            </element>
        </sequence>
    </group>
    <complexType name="FlightConditionCombinationPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:FlightConditionCombination"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractFlightConditionCombinationExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="FlightConditionElement" type="aixm:FlightConditionElementType">
        <annotation>
            <appinfo>
                <gml:description>An operand in a flight condition combination.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="FlightConditionElementType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:FlightConditionElementPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractFlightConditionElementExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="FlightConditionElementPropertyGroup">
        <sequence>
            <element minOccurs="0" name="index" nillable="true" type="aixm:NoSequenceType">
                <annotation>
                    <appinfo>
                        <gml:description>A sequence number used to indicate the order of the operand when composing the FlightConditionCombination.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="flightLevel" nillable="true" type="aixm:FlightRestrictionLevelPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>Flight level associated with a Flight Condition Element.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0"
                name="operationalCondition" nillable="true" type="aixm:FlightConditionCircumstancePropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>A condition based on operational circumstances.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <choice>
                <annotation>
                    <appinfo>
                        <gml:description>A condition based on a flight condition.</gml:description>
                    </appinfo>
                </annotation>
                <element minOccurs="0" name="flightCondition_weather"
                    nillable="true" type="aixm:MeteorologyPropertyType"/>
                <element minOccurs="0" name="flightCondition_aircraft"
                    nillable="true" type="aixm:AircraftCharacteristicPropertyType"/>
                <element minOccurs="0" name="flightCondition_flight"
                    nillable="true" type="aixm:FlightCharacteristicPropertyType"/>
                <element minOccurs="0"
                    name="flightCondition_directFlightCondition"
                    nillable="true" type="aixm:DirectFlightPropertyType"/>
                <element minOccurs="0"
                    name="flightCondition_borderCrossingCondition"
                    nillable="true" type="aixm:AirspaceBorderCrossingPropertyType"/>
                <element minOccurs="0" name="flightCondition_operand"
                    nillable="true" type="aixm:FlightConditionCombinationPropertyType"/>
                <element minOccurs="0"
                    name="flightCondition_routePortionCondition"
                    nillable="true" type="aixm:RoutePortionPropertyType"/>
                <element minOccurs="0"
                    name="flightCondition_standardInstrumentDepartureCondition"
                    nillable="true" type="aixm:StandardInstrumentDeparturePropertyType"/>
                <element minOccurs="0"
                    name="flightCondition_standardInstrumentArrivalCondition"
                    nillable="true" type="aixm:StandardInstrumentArrivalPropertyType"/>
                <element minOccurs="0"
                    name="flightCondition_organisationCondition"
                    nillable="true" type="aixm:OrganisationAuthorityPropertyType"/>
                <choice>
                    <annotation>
                        <appinfo>
                            <gml:description>A condition based on a SignificantPoint.</gml:description>
                        </appinfo>
                    </annotation>
                    <element minOccurs="0"
                        name="flightCondition_significantPointCondition_fixDesignatedPoint"
                        nillable="true" type="aixm:DesignatedPointPropertyType"/>
                    <element minOccurs="0"
                        name="flightCondition_significantPointCondition_navaidSystem"
                        nillable="true" type="aixm:NavaidPropertyType"/>
                    <element minOccurs="0"
                        name="flightCondition_significantPointCondition_position"
                        nillable="true" type="aixm:PointPropertyType"/>
                    <element minOccurs="0"
                        name="flightCondition_significantPointCondition_runwayPoint"
                        nillable="true" type="aixm:RunwayCentrelinePointPropertyType"/>
                    <element minOccurs="0"
                        name="flightCondition_significantPointCondition_aimingPoint"
                        nillable="true" type="aixm:TouchDownLiftOffPropertyType"/>
                    <element minOccurs="0"
                        name="flightCondition_significantPointCondition_airportReferencePoint"
                        nillable="true" type="aixm:AirportHeliportPropertyType"/>
                </choice>
                <element minOccurs="0"
                    name="flightCondition_airspaceCondition"
                    nillable="true" type="aixm:AirspacePropertyType"/>
                <element minOccurs="0"
                    name="flightCondition_airportHeliportCondition"
                    nillable="true" type="aixm:AirportHeliportPropertyType"/>
                <element minOccurs="0"
                    name="flightCondition_aerialRefuellingCondition"
                    nillable="true" type="aixm:AerialRefuellingPropertyType"/>
            </choice>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="FlightConditionElementPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:FlightConditionElement"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractFlightConditionElementExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <complexType name="FlightRestrictionPropertyType">
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
    </complexType>
    <element name="FlightRestriction"
        substitutionGroup="aixm:AbstractAIXMFeature" type="aixm:FlightRestrictionType">
        <annotation>
            <appinfo>
                <gml:description>A rule meant to regulate the use of the route network, by identifying a set of flights which fulfil a combination of elementary flow conditions and either forbidding them on a particular routing or obliging them to follow one routing out of a set of mandatory alternatives.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="FlightRestrictionType">
        <complexContent>
            <extension base="aixm:AbstractAIXMFeatureType">
                <sequence>
                    <element maxOccurs="unbounded" name="timeSlice" type="aixm:FlightRestrictionTimeSlicePropertyType"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="FlightRestrictionTimeSlicePropertyType">
        <sequence>
            <element ref="aixm:FlightRestrictionTimeSlice"/>
        </sequence>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="FlightRestrictionTimeSlice"
        substitutionGroup="gml:AbstractTimeSlice" type="aixm:FlightRestrictionTimeSliceType"/>
    <complexType name="FlightRestrictionTimeSliceType">
        <complexContent>
            <extension base="aixm:AbstractAIXMTimeSliceType">
                <sequence>
                    <group ref="aixm:FlightRestrictionPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <sequence>
                                <element ref="aixm:AbstractFlightRestrictionExtension"/>
                            </sequence>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="FlightRestrictionPropertyGroup">
        <sequence>
            <element minOccurs="0" name="designator" nillable="true" type="aixm:CodeFlightRestrictionDesignatorType">
                <annotation>
                    <appinfo>
                        <gml:description>A coded value which identifies the restriction.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="type" nillable="true" type="aixm:CodeFlightRestrictionType">
                <annotation>
                    <appinfo>
                        <gml:description>A code indicating if the restriction is an interdiction or a mandatory routing.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="instruction" nillable="true" type="aixm:TextInstructionType">
                <annotation>
                    <appinfo>
                        <gml:description>Some free text to describe the operational instructions which form the FlightRestriction.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="regulatedRoute" nillable="true" type="aixm:FlightRestrictionRoutePropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>The Route regulated by the Flight Restriction</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="1" minOccurs="0" name="flight"
                nillable="true" type="aixm:FlightConditionCombinationPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>The Flight affected by the Flight Restriction.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <element abstract="true" name="AbstractFlightRestrictionExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="FlightRestrictionLevel" type="aixm:FlightRestrictionLevelType">
        <annotation>
            <appinfo>
                <gml:description>A single level or a level band associated with a flight restriction, either condition or routing element.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="FlightRestrictionLevelType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:FlightRestrictionLevelPropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractFlightRestrictionLevelExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="FlightRestrictionLevelPropertyGroup">
        <sequence>
            <element minOccurs="0" name="upperLevel" nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <appinfo>
                        <gml:description>The value of the uppermost cruising level.&#xd;
The data type also allows a special non-numerical value "CEILING", meaning "the top of the airspace. This can useful in the case of Airspace that have a non-constant upper limit.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="upperLevelReference"
                nillable="true" type="aixm:CodeVerticalReferenceType">
                <annotation>
                    <appinfo>
                        <gml:description>The reference surface used for the value of the upper level. For example, Mean Sea Level, Ground, standard pressure,  etc..</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="lowerLevel" nillable="true" type="aixm:ValDistanceVerticalType">
                <annotation>
                    <appinfo>
                        <gml:description>The value of the lowermost cruising level.&#xd;
The data type also allows a special non-numerical value "FLOOR" meaning "the bottom of the airspace".  This can useful in the case of Airspace that have a non-constant lower limit.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="lowerLevelReference"
                nillable="true" type="aixm:CodeVerticalReferenceType">
                <annotation>
                    <appinfo>
                        <gml:description>The reference surface used for the value of the lower level. For example, Mean Sea Level, Ground, standard pressure,  etc..</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="FlightRestrictionLevelPropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:FlightRestrictionLevel"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractFlightRestrictionLevelExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="FlightRestrictionRoute" type="aixm:FlightRestrictionRouteType">
        <annotation>
            <appinfo>
                <gml:description>A sequence of routing elements (points, route portions or airspace) that must/must not appear in the specified order in the flight plan of an aircraft fulfilling the conditions of the related Flight Restriction (according to its type attribute).&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="FlightRestrictionRouteType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
                    <group ref="aixm:FlightRestrictionRoutePropertyGroup"/>
                    <element maxOccurs="unbounded" minOccurs="0" name="extension">
                        <complexType>
                            <choice>
                                <element ref="aixm:AbstractFlightRestrictionRouteExtension"/>
                            </choice>
                            <attributeGroup ref="gml:OwnershipAttributeGroup"/>
                        </complexType>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <group name="FlightRestrictionRoutePropertyGroup">
        <sequence>
            <element minOccurs="0" name="priorPermission"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <appinfo>
                        <gml:description>The minimum lead time required for contacting the relevant authority in order to get permission for using the routing.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="routeElement" nillable="true" type="aixm:FlightRoutingElementPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>Routing element concerned by a specified flight restriction.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0" name="contact"
                nillable="true" type="aixm:ContactInformationPropertyType">
                <annotation>
                    <appinfo>
                        <gml:description>The contact information of the relevant authority which can grant the permission for using the routing.</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element maxOccurs="unbounded" minOccurs="0"
                name="annotation" nillable="true" type="aixm:NotePropertyType"/>
        </sequence>
    </group>
    <complexType name="FlightRestrictionRoutePropertyType">
        <complexContent>
            <extension base="aixm:AbstractAIXMPropertyType">
                <sequence>
                    <element ref="aixm:FlightRestrictionRoute"/>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <element abstract="true"
        name="AbstractFlightRestrictionRouteExtension"
        substitutionGroup="aixm:AbstractExtension" type="aixm:AbstractExtensionType"/>
    <element name="FlightRoutingElement" type="aixm:FlightRoutingElementType">
        <annotation>
            <appinfo>
                <gml:description>An airspace, route or point that is concerned by a specified flight restriction.&#xd;
</gml:description>
            </appinfo>
        </annotation>
    </element>
    <complexType name="FlightRoutingElementType">
        <complexContent>
            <extension base="aixm:AbstractAIXMObjectType">
                <sequence>
