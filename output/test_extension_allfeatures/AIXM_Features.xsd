                    <appinfo>
                        <gml:description>The guidance equipment which is inoperative</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="landingSystemLights"
                nillable="true" type="aixm:CodeYesNoType">
                <annotation>
                    <appinfo>
                        <gml:description>Indicates if landing System Lights are available</gml:description>
                    </appinfo>
                </annotation>
            </element>
            <element minOccurs="0" name="equipmentRVR" nillable="true" type="aixm:CodeYesNoType">
